    <RepositoryUrl>https://github.com/dejvids/WPFNet5.Core</RepositoryUrl>
    <Authors>Dawid Suryś</Authors>
    <Company />
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
    <AssemblyVersion>0.1.0.0</AssemblyVersion>
